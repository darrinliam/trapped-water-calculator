{
    "env": {
		"es6": true,
		"browser": true
    },
    "extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
		"eqeqeq": 1,
		"no-var": "error",
		"no-mixed-spaces-and-tabs": "off",
		"react/prop-types": "off"
    }
}
